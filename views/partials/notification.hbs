    <section class="container">
    {{#each list}}
    <div class="row py-3 w-100">
  <div class="col">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title">{{this.fullname}}</h5>
        <div>
        <h5 class="card-title">{{this.matriculation}}</h5>
        </div>
        <p class="card-text">{{this.compfield}}</p>
        <div class="d-flex">
        <form action="/admin/notificationedit" method="POST">
           <button class="btn btn btn-primary mx-2" name="id" value={{this._id}}  type="submit">Edit</button>
          </form>
          <form action="/admin/complaindelete" method="POST">
           <button class="btn btn-secondary" name="id" value={{this._id}}  type="submit">delete</button>
          </form>
          </div>
          
      </div>
    </div>
  </div>
{{/each}}
</section>